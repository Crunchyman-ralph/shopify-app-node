{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "./node_modules/.cache/dist",
    "tsBuildInfoFile": "./node_modules/.cache/.tsbuildinfo",
    "noImplicitAny": false,
    "esModuleInterop": true,
    "module": "commonjs",
    "paths": {
      "api/*": ["./*"],
      "web-app/*": ["../web-app/src/*"]
    }
  },
  "include": ["src/**/*"],
  "references": [
    {
      "path": "../web-app"
    }
  ]
}
