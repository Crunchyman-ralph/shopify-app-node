{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "./node_modules/.cache/dist",
    "tsBuildInfoFile": "./node_modules/.cache/.tsbuildinfo",
    "paths": {
      "web-app/*": [
        "./src/*"
      ]
    },
    "rootDir": "./src"
  },
  "include": [
    "./src/**/*.ts",
    "./src/**/*.tsx",
    "./src/**/*.js",
    "./src/**/*.json",
    "./**/*.ts",
    "./**/*.tsx",
    "./**/*.js",
    "./**/*.json",
    "*.json"
  ],
  "references": []
}